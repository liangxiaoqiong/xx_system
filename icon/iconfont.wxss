@font-face {
    font-family: 'myicon';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
    url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
    url('myicon.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

[class^="myicon-"], [class*=" myicon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'myicon' !important;
    speak: none;
    font-size: 28rpx;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.myicon-tip_grey:before {
    content: "\e903";
    color: #9f9f9f;
}
.myicon-tip_red:before {
    content: "\e903";
    color: #f82e2e;
}
.myicon-selected2:before {
    content: "\e900";
    color: #f82e2e;
}
.myicon-minus:before {
    content: "\e901";
    color: #f82f2f;
}
.myicon-minus.disabled:before,
.myicon-add.disabled:before{
    color: #C9C9C9;
}
.myicon-add:before {
    content: "\e902";
    color: #f82e2e;
}
.myicon-close:before {
    content: "\e904";
    color: #999;
}
.myicon-sort_desc .path1:before {
    content: "\e905";
    color: rgb(153, 153, 153);
}
.myicon-sort_desc .path2:before {
    content: "\e906";
    margin-left: -1em;
    color: rgb(248, 46, 46);
}
.myicon-selected:before {
    content: "\e907";
    color: #f82f2f;
}
.myicon-sort_normal .path1:before {
    content: "\e905";
    color: rgb(153, 153, 153);
}
.myicon-sort_normal .path2:before {
    content: "\e906";
    margin-left: -1em;
    color: rgb(153, 153, 153);
}
.myicon-circle:before {
    content: "\e90b";
    color: #999;
}
.myicon-screen:before {
    content: "\e90f";
    color: #999;
}
.myicon-sort_asc .path1:before {
    content: "\e912";
    color: rgb(248, 46, 46);
}
.myicon-sort_asc .path2:before {
    content: "\e913";
    margin-left: -1em;
    color: rgb(153, 153, 153);
}
